{
  "compilerOptions": {
    "target": "ES2020",
    "module": "commonjs",
    "outDir": "./dist",
    "rootDir": "./src",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true
  },
  "include": [
    "src/**/*.ts"
, "src/app.ts", "src/ollama.ts", "src/queue.ts", "src/config.ts"  ],
  "exclude": [
    "node_modules",
    "**/*.test.ts"
  ]
}